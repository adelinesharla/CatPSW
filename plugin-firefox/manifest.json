{
    "manifest_version": 2,
    "name": "Cat Password",
    "version": "1.0",
    "description": "Gerenciador de senhas",
    "icons": {
      "26": "icon.png"
    },
    "background": {
      "scripts": ["scripts/background.js"],
      "persistent": true
    },
    "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "pages/login.html"
    },
    "sidebar_action": {
      "default_title": "CatPSW",
      "default_panel": "pages/sidebar.html"
    },
  
    "permissions": [
      "tabs",
      "activeTab",
      "storage",
      "<all_urls>" 
    ]
  }